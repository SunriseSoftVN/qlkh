<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.qlvt.core.client.model.SubTaskAnnualDetail" table="subtaskannualdetail" schema="" catalog="qlkh">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="createdDate" column="createdDate" not-null="true"/>
        <property name="updatedDate" column="updatedDate" not-null="true"/>
        <property name="createBy" column="createBy" not-null="true"/>
        <property name="updateBy" column="updateBy" not-null="true"/>

        <property name="lastYearValue" column="lastYearValue"/>
        <property name="increaseValue" column="increaseValue"/>
        <property name="decreaseValue" column="decreaseValue"/>
        <property name="realValue" column="realValue"/>

        <many-to-one name="taskDetail" class="com.qlvt.core.client.model.TaskDetail" lazy="false" fetch="join" outer-join="auto">
            <column name="taskDetailId" not-null="true"/>
        </many-to-one>
        <many-to-one name="branch" class="com.qlvt.core.client.model.Branch" lazy="false" fetch="join" outer-join="true">
            <column name="branchId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>