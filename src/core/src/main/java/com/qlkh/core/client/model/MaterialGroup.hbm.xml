<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.qlkh.core.client.model.MaterialGroup" table="material_group" schema="" catalog="qlkh">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="createdDate" column="createdDate" not-null="true"/>
        <property name="updatedDate" column="updatedDate" not-null="true"/>
        <property name="createBy" column="createBy" not-null="true"/>
        <property name="updateBy" column="updateBy" not-null="true"/>

        <property name="code" column="code" not-null="true"/>
        <property name="name" column="name" not-null="true"/>
        <property name="regex" column="regex"/>
        <property name="codeDisplay" column="codeDisplay" not-null="true"/>
        <property name="position" column="position" not-null="true"/>
    </class>
</hibernate-mapping>