<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dispatch" class="net.customware.gwt.dispatch.server.spring.SpringDispatch">
        <constructor-arg index="0" ref="registry"/>
    </bean>

    <bean id="registry" class="net.customware.gwt.dispatch.server.DefaultActionHandlerRegistry">
        <property name="actionHandlers">
            <list>
                <bean class="com.qlvt.server.handler.LoginHandler"/>
                <bean class="com.qlvt.server.handler.LoadGridDataHandler"/>
                <bean class="com.qlvt.server.handler.LoadHandler"/>
                <bean class="com.qlvt.server.handler.SaveHandler"/>
                <bean class="com.qlvt.server.handler.DeleteHandler"/>
                <bean class="com.qlvt.server.handler.ReportHandler"/>
                <bean class="com.qlvt.server.handler.LoadStationHandler"/>
                <bean class="com.qlvt.server.handler.LoadSubTaskAnnualHandler"/>
                <bean class="com.qlvt.server.handler.LoadTaskAnnualDetailHandler"/>
                <bean class="com.qlvt.server.handler.LoadAnnualTaskHandler"/>
                <bean class="com.qlvt.server.handler.DeleteTaskDetailHandler"/>
                <bean class="com.qlvt.server.handler.LoadTaskDetailHandler"/>
                <bean class="com.qlvt.server.handler.LoadNormalTaskHandler"/>
                <bean class="com.qlvt.server.handler.LoadSubTaskDetailHandler"/>
                <bean class="com.qlvt.server.handler.time.GetServerTimeHandler"/>
            </list>
        </property>
    </bean>

</beans>