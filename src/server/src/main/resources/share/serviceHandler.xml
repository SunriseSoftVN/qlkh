<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dispatch" class="net.customware.gwt.dispatch.server.spring.SpringDispatch">
        <constructor-arg index="0" ref="registry"/>
    </bean>

    <bean id="registry" class="net.customware.gwt.dispatch.server.DefaultActionHandlerRegistry">
        <property name="actionHandlers">
            <list>
                <bean class="com.qlvt.server.handler.core.LoginHandler"/>
                <bean class="com.qlvt.server.handler.gird.LoadGridDataHandler"/>
                <bean class="com.qlvt.server.handler.core.LoadHandler"/>
                <bean class="com.qlvt.server.handler.core.SaveHandler"/>
                <bean class="com.qlvt.server.handler.core.DeleteHandler"/>
                <bean class="com.qlvt.server.handler.report.ReportHandler"/>
                <bean class="com.qlvt.server.handler.station.LoadStationHandler"/>
                <bean class="com.qlvt.server.handler.subtask.LoadSubTaskAnnualHandler"/>
                <bean class="com.qlvt.server.handler.taskdetail.DeleteTaskDetailHandler"/>
                <bean class="com.qlvt.server.handler.subtask.LoadSubTaskDetailHandler"/>
                <bean class="com.qlvt.server.handler.task.LoadUnusedTaskGridHandler"/>
                <bean class="com.qlvt.server.handler.time.GetServerTimeHandler"/>
                <bean class="com.qlvt.server.handler.station.LockStationHandler"/>
            </list>
        </property>
    </bean>

</beans>